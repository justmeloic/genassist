# Gemini API Configuration
# GEMINI_API_KEY is now optional - users provide it at login
GEMINI_API_KEY=""
GEMINI_MODEL_DOCUMENT="gemini-2.0-flash"
GEMINI_MODEL_TTS="gemini-2.5-flash-preview-tts"
GEMINI_MODEL_MULTI_TTS="gemini-2.5-flash-preview-tts"
GEMINI_MODEL_VIDEO="veo-2.0-generate-001"
GEMINI_MODEL_IMAGE="imagen-4.0-generate-preview-06-06"
GEMINI_MODEL_LIVE_HALF_CASCADE="gemini-live-2.5-flash-preview"
GEMINI_MODEL_LIVE_NATIVE_AUDIO="gemini-2.5-flash-preview-native-audio-dialog"
GEMINI_MODEL_LIVE_THINKING="gemini-2.5-flash-exp-native-audio-thinking-dialog"

# Security
SECRET_KEY=your_secret_key_here
AUTH_SECRET=your_auth_secret_here

# App Configuration
PROJECT_NAME="Document Service API"
VERSION="1.0.0"
DEBUG=true
ENVIRONMENT=development

# Audio Configuration
AUDIO_SAMPLE_RATE=24000
AUDIO_CHANNELS=1
AUDIO_SAMPLE_WIDTH=2
DEFAULT_VOICE=Kore

# File Configuration
AUDIO_OUTPUT_DIR="./content/audio"
VIDEO_OUTPUT_DIR="./content/video"
IMAGE_OUTPUT_DIR="./content/image"
MAX_FILE_SIZE=10485760

# Security (optional for testing)
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30